
pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Operacion'
            }
        }
        stage('Clone Repository') {
            steps {
                git branch: 'main', url: 'https://github.com/Velascoudea/Demo.git'
            }
        }
         stage('Compile Java') {
            steps {
                script {
                    bat "javac Operaciones.java" 
                }
                script {
                    bat "java Operaciones" 
                }
            }
        }
    }
}